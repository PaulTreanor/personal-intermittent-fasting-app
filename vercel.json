{
    "routes": [
      {
        "src": "/.*",
        "has": [
          {
            "type": "header",
            "key": "authorization",
            "value": "Basic <?env AUTH_PAIR?>"
          }
        ],
        "continue": true
      },
      {
        "src": "/.*",
        "headers": {
          "www-authenticate": "Basic realm='Secure Area'"
        },
        "status": 401
      }
    ]
  }
  